
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/EstateAgent/Views/LayoutEstateAgent/Index.cshtml";
}

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">

        <!-- Sale & Revenue Start -->
        @await Component.InvokeAsync("_EstateAgentDashboardStatisticComponentPartial")
       
        <!-- Sales Chart Start -->
        @await Component.InvokeAsync("_EstateAgentDashboardChartComponentPartial")
        <!-- Sale & Revenue End -->
        <!-- Sales Chart End -->
        <!-- Recent Sales Start -->
        @await Component.InvokeAsync("_EstateAgentLastFiveProductComponentPartial")
        <!-- Recent Sales End -->
        <!-- Widgets Start -->
        <div class="container-fluid pt-4 px-4">
            <div class="row g-4">

                @await Component.InvokeAsync("_DashboardLastContactListComponentPartial")

                <div class="col-sm-12 col-md-6 col-xl-4">
                    <div class="h-100 bg-light rounded p-4">
                        <div class="d-flex align-items-center justify-content-between mb-4">
                            <h6 class="mb-0">Calender</h6>
                            <a href="">Show All</a>
                        </div>
                        <div id="calender"></div>
                    </div>
                </div>
                @await Component.InvokeAsync("_DashboardToDoListComponentPartial")


            </div>
        </div>

    </div>
</div>

